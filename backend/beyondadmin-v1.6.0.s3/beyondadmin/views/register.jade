  // Body
  body
    .register-container.animated.fadeInDown
      .registerbox.bg-white
        .registerbox-title Register
        |             
        .registerbox-caption Please fill in your information
        |             
        .registerbox-textbox
          input.form-control(type='text', placeholder='Username')
        |             
        .registerbox-textbox
          input.form-control(type='password', placeholder='Enter Password')
        |             
        .registerbox-textbox
          input.form-control(type='password', placeholder='Confirm Password')
        |             
        hr.wide
        |             
        .registerbox-textbox
          input.form-control(type='text', placeholder='Name')
        |             
        .registerbox-textbox
          input.form-control(type='text', placeholder='Family')
        |             
        .registerbox-textbox
          .row
            .col-lg-6.col-sm-6.col-xs-6.padding-right-10
              input.form-control(type='text', placeholder='Month')
            |                     
            .col-lg-3.col-sm-3.col-xs-3.no-padding.padding-right-10
              input.form-control(type='text', placeholder='Day')
            |                     
            .col-lg-3.col-sm-3.col-xs-3.no-padding-left
              input.form-control(type='text', placeholder='Year')
        |             
        .registerbox-textbox.no-padding-bottom
          .checkbox
            label
              input.colored-primary(type='checkbox', checked='checked')
              |                         
              span.text.darkgray
                | I agree to the Company 
                a.themeprimary Terms of Service
                |  and Privacy Policy
        |             
        .registerbox-submit
          input.btn.btn-primary.pull-right(type='button', value='SUBMIT')
      |         
      .logobox
    // Basic Scripts
    script(src='assets/js/jquery.min.js')
    |     
    script(src='assets/js/bootstrap.min.js')
    |     
    script(src='assets/js/slimscroll/jquery.slimscroll.min.js')
